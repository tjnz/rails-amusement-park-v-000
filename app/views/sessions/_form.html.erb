<%= form_for @user, url: url_for(controller: 'sessions', action: 'create') do |f| %>
	<div class="field">
		<%= f.label "Select your name" %><br>
		<%= f.collection_select(:id, @users, :id, :name, :include_blank => true )%>
	</div>
	
	<div class="actions">
		<%= f.submit "Sign In" %>
	</div>
	
<% end %>